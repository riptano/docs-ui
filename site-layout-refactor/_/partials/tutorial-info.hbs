<div class="flex flex-wrap items-center gap-1 mb-3 -mt-1 is-before-toc">
  {{#with page.attributes.skill-level}}
    <div class="text-teritiary flex gap-0.5 text-sm">
      <span class="material-icons">network_check</span>
      {{{this}}}
    </div>
  {{/with}}
  {{#with (stepcount page.contents)}}
    <div class="text-teritiary flex gap-0.5 text-sm">
      <span class="material-icons">playlist_add_check</span>
      {{{this}}}
      Steps
    </div>
  {{/with}}
  {{#with page.attributes.time-commitment}}
    <div class="text-teritiary flex gap-0.5 text-sm">
      <span class="material-icons">query_builder</span>
      {{{this}}}
    </div>
  {{/with}}
  <div class="ml-auto">
    {{#with page.attributes.colab-link}}
      <a
        href="{{{this}}}"
        target="_blank"
        class="btn btn-outlined btn-neutral after:hidden"
        data-track="Colab Link Clicked"
      >
        <i class="mr-1 h-[1em] w-[1.5em] bg-[url('../img/colab.svg')] bg-center bg-no-repeat"></i>
        Run this on Colab
      </a>
    {{/with}}
  </div>
</div>